<?php
/**
 * @file
 * Code for the twitter_import feature.
 */

include_once 'twitter_import.features.inc';

function twitter_import_page_alter(&$page) {

  $current_path = arg(0);

  drupal_add_js(array('twitterImport' => array(
    'currentPath' => $current_path,
  )), array('type' => 'setting'));
}
